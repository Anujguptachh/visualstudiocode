@model IEnumerable<Libarary_Management_Code.Models.PENDINGPAYMENT>

<p>
    @Html.ActionLink("Back", "Index")
</p>
<form id="formSearch" method="post">
    <dev class="row" id="SearchBox">
        <div class="col-md-2">Name</div>
        <div class="col-md-2"><input type="text" id="searching" name="searching" /></div>
        <div class="col-md-2"><input type="submit" id="submitsearch" name="Search" /></div>
    </dev>
</form>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mobile)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastpaymentDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Duration)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mobile)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastpaymentDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Duration)
        </td>
    </tr>
}

</table>
